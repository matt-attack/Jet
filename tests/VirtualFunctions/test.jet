
extern_c fun int printf(char* h, int h);
extern_c fun char* malloc(int x);

extern_c fun int puts(char* str);

extern_c fun char getchar();

struct A
{
	int a_val;
	
	virtual void hi()
	{
		puts("I'm A\n");
	}
	
	virtual void hi2()
	{
		puts("I'm A\n");
	}
}

struct B: A
{
	char p;
	virtual void hi()
	{
		puts("I'm B\n");
	}
}

struct C: B
{
	int q;
	virtual void hi()
	{
		puts("I'm C\n");
	}
	
	virtual void hi2()
	{
		puts("I'm C\n");
	}
}

fun int main()
{
	let B b;
	let A a;
	let C c;
	
	a.hi();
	b.hi();
	c.hi();
	
	let A* bp = &b;
	bp.hi();
	bp.hi2();
	
	let B* cp = &c;
	cp.hi();
	cp.hi2();
	
	let A* cp2 = &c;
	cp2.hi();
	cp2.hi2();

	getchar();
	return 0;
}