struct Vector3
{
    float x;
    float y;
    float z;

    fun void Vector3()
    {
        this.x = 0.0;
        this.y = 0.0;
        this.z = 0.0;
    }

    fun void Vector3(float x, float y, float z)
    {
        this.x = x;
        this.y = y;
        this.z = z;
    }

    fun Vector3 operator+(Vector3 v)
    {
        return Vector3(this.x + v.x, this.y + v.y, this.z + v.z);
    }

    fun bool operator==(Vector3 v)
    {
        return this.x == v.x || this.y == v.y || this.z == v.z;
    }
}

