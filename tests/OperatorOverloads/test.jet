struct A
{
	int a_val;
}

struct B
{
	int b_val;
	
	fun B operator +(int other)
	{
		let B res;
		res.b_val = this->b_val+other;
		return res;
	}
	
	fun B operator -(int other)
	{
		let B res;
		res.b_val = this->b_val-other;
		return res;
	}
	
	fun B operator*(int other)
	{
		let B res;
		res.b_val = this->b_val*other;
		return res;
	}
	
	fun B operator/(int other)
	{
		let B res;
		res.b_val = this->b_val/other;
		return res;
	}
}

union C = A | B;

fun int main() 
{
